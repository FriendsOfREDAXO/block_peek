{
  "require": {
    "php": ">=8.2",
    "symfony/cache": "^5.4"
  },
  "replace": {
    "psr/log": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "autoload": {
    "classmap": [
      "lib/"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "platform": {
      "php": "8.3.24"
    },
    "platform-check": false
  }
}
